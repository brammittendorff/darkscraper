      "title": "Unique Infrastructure Patterns",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 8,
        "y": 0
      },
      "datasource": {
        "type": "grafana-postgresql-datasource",
        "uid": "darkscraper-pg"
      },
      "targets": [
        {
          "rawSql": "SELECT COUNT(DISTINCT value) FROM correlations;",
